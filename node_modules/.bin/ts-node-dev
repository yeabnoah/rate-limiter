#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/technerd/Documents/projects/side_project/rate-limiter/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.17.12_typescript@5.7.2/node_modules/ts-node-dev/lib/node_modules:/home/technerd/Documents/projects/side_project/rate-limiter/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.17.12_typescript@5.7.2/node_modules/ts-node-dev/node_modules:/home/technerd/Documents/projects/side_project/rate-limiter/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.17.12_typescript@5.7.2/node_modules:/home/technerd/Documents/projects/side_project/rate-limiter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/technerd/Documents/projects/side_project/rate-limiter/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.17.12_typescript@5.7.2/node_modules/ts-node-dev/lib/node_modules:/home/technerd/Documents/projects/side_project/rate-limiter/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.17.12_typescript@5.7.2/node_modules/ts-node-dev/node_modules:/home/technerd/Documents/projects/side_project/rate-limiter/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@20.17.12_typescript@5.7.2/node_modules:/home/technerd/Documents/projects/side_project/rate-limiter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node-dev/lib/bin.js" "$@"
else
  exec node  "$basedir/../ts-node-dev/lib/bin.js" "$@"
fi
